{
  "name": "ahrefs",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "preinstall:opam": "opam update",
    "install:opam": "opam install -y . --deps-only --with-test",
    "install:npm-opam": "npm install && npm run install:opam",
    "init": "opam switch create . 5.1.1 -y --deps-only && npm run install:npm-opam",
    "build": "dune build",
    "start": "dune build && concurrently 'npm:vite:dev' 'npm:dune:watch'",
    "ngrok": "ngrok http 5173 --host-header=\"localhost:5173\" --subdomain=designsbysm",
    "dune:watch": "dune build -w",
    "serve": "dune build && vite preview",
    "vite:dev": "vite _build/default --open"
  },
  "dependencies": {
    "flag-icons": "^7.2.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-select": "^5.8.0",
    "react-window": "^1.8.10"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "concurrently": "^8.2.2",
    "vite": "^4.5.0",
    "vite-plugin-melange": "^2.2.0"
  }
}
